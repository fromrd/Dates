<div class="content_wrapper">
    <%= form_with(model: @review, local: true, url: reviews_path ) do |form| %>
      <%= form.hidden_field :plan_id, value: params[:plan_id] %>
      <p><%= form.label :review %></p>
      <p><%= form.text_field :comment %></p>
      <p><%= form.submit %></p>
    <% end %>
</div>